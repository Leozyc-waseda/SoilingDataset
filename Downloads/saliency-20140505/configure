  enable_ldsortcommon=yes
fi


   { $as_echo "$as_me:${as_lineno-$LINENO}: result: $enable_ldsortcommon" >&5
$as_echo "$enable_ldsortcommon" >&6; }
else
   enable_ldsortcommon=no
fi

