# MPI libraries:
MPI_LIBS=""
for ac_prog in mpic++
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_MPICC_PROG+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $MPICC_PROG in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_MPICC_PROG="$MPICC_PROG" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_MPICC_PROG="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
MPICC_PROG=$ac_cv_path_MPICC_PROG
if test -n "$MPICC_PROG"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MPICC_PROG" >&5
$as_echo "$MPICC_PROG" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$MPICC_PROG" && break
done

if test "x$MPICC_PROG" = "x"; then
   for ac_prog in mpiCC
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_MPICC_PROG+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $MPICC_PROG in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_MPICC_PROG="$MPICC_PROG" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_MPICC_PROG="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
MPICC_PROG=$ac_cv_path_MPICC_PROG
if test -n "$MPICC_PROG"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MPICC_PROG" >&5
$as_echo "$MPICC_PROG" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$MPICC_PROG" && break
done

fi

if test "x$MPICC_PROG" = "x"; then
   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: mpic++/mpiCC program missing; MPI programs won't be able to be built" >&5
$as_echo "$as_me: WARNING: mpic++/mpiCC program missing; MPI programs won't be able to be built" >&2;}
late_warnings="${late_warnings}
* mpic++/mpiCC program missing; MPI programs won't be able to be built"
else
   libs=`$MPICC_PROG -showme:link 2> /dev/null`
   status=$?
   if test $status -ne 0; then
      libs=`$MPICC_PROG -show 2> /dev/null`
      status=$?
   fi
   if test $status -eq 0; then
      for arg in $libs; do
          case $arg in
             -l*)
                MPI_LIBS="$MPI_LIBS $arg"
                ;;
             -L*)
                LDFLAGS="$LDFLAGS $arg"
                ;;
          esac
      done
   fi
   { $as_echo "$as_me:${as_lineno-$LINENO}: checking for MPI libraries" >&5
$as_echo_n "checking for MPI libraries... " >&6; }
   { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MPI_LIBS" >&5
$as_echo "$MPI_LIBS" >&6; }

$as_echo "#define INVT_HAVE_MPI 1" >>confdefs.h

fi


