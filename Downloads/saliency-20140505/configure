                    QT3DIR=/usr
                    QT3_INC_DIR="/usr/include/$d"
                fi
            done
	else
            if test -f $QT3DIR/include/qglobal.h; then
                QT3_INC_DIR="$QT3DIR/include"
            fi
        fi
        ;;

    *)
        # the user gave a --with-qt3dir=value, so let's use that value
        QT3DIR=$with_qt3dir
